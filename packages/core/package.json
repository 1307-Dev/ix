{
  "name": "@siemens/ix",
  "version": "0.0.0",
  "license": "Siemens Inner Source License v1.4",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/core-ui-core/core-ui-core.js",
  "files": [
    "dist/",
    "loader/",
    "src/**/*.md",
    "scss/"
  ],
  "scripts": {
    "build": "stencil build --docs --prod",
    "build:dev": "stencil build --docs",
    "lint": "eslint src/**/*{.ts,.tsx}",
    "start": "stencil build --dev --watch --serve",
    "dev": "stencil build --watch",
    "test": "stencil test --spec",
    "test.watch": "stencil test --spec --watchAll --no-cache",
    "test.coverage": "stencil test --spec --coverage",
    "test.e2e": "playwright test --reporter html",
    "test.e2e.list": "playwright test --reporter list",
    "host-root": "http-server ./ -a 127.0.0.1 -p 8080",
    "generate": "stencil generate",
    "postversion": "echo \"/*\n * COPYRIGHT (c) Siemens AG 2018-2022 ALL RIGHTS RESERVED.\n */\nexport const version = '$npm_package_version'\" > ./src/env.ts",
    "generate.theme": "ts-node -P ./scripts/tsconfig.json ./scripts/generate-theme"
  },
  "dependencies": {
    "@siemens/core-ui-icons": "~1.1.16",
    "@stencil/core": "~2.15.0",
    "animejs": "~3.2.1",
    "hyperlist": "^1.0.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.24.1",
    "@popperjs/core": "~2.11.0",
    "@siemens/core-ui-icons": "^1.1.16",
    "@stencil/angular-output-target": "^0.4.0",
    "@stencil/eslint-plugin": "^0.4.0",
    "@stencil/postcss": "^2.0.0",
    "@stencil/react-output-target": "^0.3.1",
    "@stencil/sass": "~1.5.2",
    "@testing-library/dom": "^8.11.0",
    "@types/animejs": "^3.1.4",
    "@types/jest": "^26.0.18",
    "@types/mustache": "^4.1.1",
    "@types/puppeteer": "^5.4.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "animate.css": "~4.1.1",
    "autoprefixer": "10.4.5",
    "bootstrap": "~5.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "fs-extra": "^10.0.0",
    "http-server": "^14.1.1",
    "jest": "^26.6.3",
    "jest-cli": "^26.6.3",
    "mustache": "^4.2.0",
    "puppeteer": "^10.0.0",
    "sass": "^1.54.0",
    "start-server-and-test": "^1.14.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "@popperjs/core": "^2.11.0",
    "bootstrap": "~5.2.0"
  },
  "publishConfig": {
    "registry": "***REMOVED***"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
