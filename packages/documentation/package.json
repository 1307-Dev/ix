{
  "name": "documentation",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "docusaurus": "docusaurus",
    "start": "npm run copy-lib && npm run generate-api && docusaurus start",
    "build": "npm run copy-lib && npm run generate-api && docusaurus build",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "clear": "docusaurus clear",
    "rimraf": "rimraf ./static/webcomponent-examples/lib/ix && rimraf ./static/webcomponent-examples/lib/core-ui-icons",
    "serve": "docusaurus serve",
    "write-translations": "docusaurus write-translations",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc",
    "copy-lib": "npm run rimraf && node ./scripts/copy-webcomponents.mjs",
    "generate-api": "node ./scripts/generate-api.mjs",
    "generate-version": "npm run generate-api && npm run copy-lib && node ./scripts/generate-version.mjs"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.1",
    "@docusaurus/preset-classic": "^2.0.1",
    "@docusaurus/theme-live-codeblock": "^2.0.1",
    "@mdx-js/react": "^1.6.22",
    "@siemens/ix": "./../core",
    "@siemens/ix-react": "./../react",
    "@stencil/postcss": "^2.1.0",
    "clsx": "^1.2.1",
    "execa": "^5.1.1",
    "prism-react-renderer": "^1.3.5",
    "raw-loader": "^4.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-toastify": "^9.0.7"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "^2.0.1",
    "@tsconfig/docusaurus": "^1.0.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=16.14"
  }
}
